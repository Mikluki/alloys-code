# Energy Sources Configuration
# Place this file at: ~/.energy_sources.yaml
#
# This file allows you to override virtual environment paths for energy sources.
# Only specify the sources you want to override - others will use defaults.

venvs:
  # Neural network models
  ESEN_30M_OAM: ~/.venvs/uv312eSEN
  MACE: ~/.venvs/uv312mace
  MACE_MPA_0: ~/.venvs/uv312mace
  SEVENNET: ~/.venvs/uv312SevenNet
  ORBV3: ~/.venvs/uv312ORBv3
  
  # Services
  MP: ~/.venvs/py312
  
